<Window x:Class="ThesisProjectARM.UI.Views.Windows.MainUIWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ThesisProjectARM.UI.Views.Windows"
        mc:Ignorable="d"
        Title="MainUIWindow" Height="450" Width="800">
    <Grid>
        <Button Content="Load Data" Command="{Binding LoadDataCommand}" />
        <Button Content="Analyze Data" Command="{Binding AnalyzeDataCommand}" />
        <Button Content="Visualize Data" Command="{Binding VisualizeDataCommand}" />
        <Button Content="Export Data" Command="{Binding ExportDataCommand}" />
        <Button Content="Save Changes" Command="{Binding SaveChangesCommand}" />
    </Grid>
</Window>
