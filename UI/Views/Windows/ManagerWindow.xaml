<Window x:Class="ThesisProjectARM.UI.Views.Windows.ManagerWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ThesisProjectARM.UI.Views.Windows"
        mc:Ignorable="d"
        Title="ManagerWindow" Height="450" Width="800">
    <Grid>
        <DataGrid x:Name="UsersDataGrid" DataContext="{Binding Username}" Style="{StaticResource DataGridStyle}" AutoGenerateColumns="True" Margin="0,115,0,0"/>
    </Grid>
</Window>
